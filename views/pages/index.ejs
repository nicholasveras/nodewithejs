<!DOCTYPE html>


    <%- include('../partials/head'); %>

<body class="container">
    <%- include('../partials/header'); %>   
    <main>
        <div class="card">
            <div class="card-body">
                <h1 class="card-title">Node com EJS</h1>
                <p class="card-text">Aprendendo a trabalhar com arquivos EJS.</p>
                <p><%= subtitle %></p>
            </div>
        </div>
        <ul>
            <% qualitys.forEach(function(quality){ %>
                <li>
                    <strong><%= quality.title %></strong> <%= quality.message %>
                </li>
            <% }); %>
            
        </ul>
    </main>   

    <%- include('../partials/footer'); %>

</body>
</html>